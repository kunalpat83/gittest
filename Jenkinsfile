pipeline {
	agent any
	stages {
		stage ('Build') {
				steps {
					echo 'This is my first Declarative Script and for Build'
				      }
				}
		
	        stage ('Test') {
			steps {
					echo 'This is my first Declarative Script for Test'
			}
		}	
		stage ('Deploy') {
			steps {
					echo 'This is my first Declarative Script for Deploy'
			}			
		}		
	}
}
